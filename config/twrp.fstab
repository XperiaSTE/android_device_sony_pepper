# mount point	fstype		device			[device2]   flags

/boot		emmc		/dev/block/mmcblk0p9
/system	ext4		/dev/block/mmcblk0p10
/cache		ext4		/dev/block/mmcblk0p12
/data		ext4		/dev/block/mmcblk0p11
/sdcard	vfat		/dev/block/mmcblk0p14	flags=display="internal_sd";storage;wipeingui
/external_sd vfat       /dev/block/mmcblk1p1   /dev/block/mmcblk1   flags=display="external_sd";storage;wipeingui;removable
/usb-otg 	vfat 	/dev/block/sda1 	/dev/block/sda flags=display="USB-OTG";storage;wipeingui;removable